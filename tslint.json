{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-debugger": false,
    "ter-arrow-parens": false,
    "import-name": false,
    "variable-name": [true, "check-format", "allow-leading-underscore"]
  },
  "jsRules": {
    //Disable for javascript
    "no-console": false,
    "object-literal-sort-keys": false,
    "no-debugger": false
  }
}
