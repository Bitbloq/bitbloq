{
  "name": "@bitbloq/api",
  "version": "1.0.0",
  "description": "## Despliegue",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__test__/*.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^2.4.3",
    "@graphql-codegen/cli": "^1.8.3",
    "@types/graphql": "^14.0.7",
    "@types/lodash": "^4.14.123",
    "@types/mongoose": "^5.3.23",
    "apollo-server": "^2.15.0",
    "apollo-server-koa": "^2.15.0",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.5.3",
    "bson": "^4.0.2",
    "dotenv": "^6.2.0",
    "googleapis": "^45.0.0",
    "graphql": "^15.1.0",
    "graphql-import": "^0.7.1",
    "graphql-redis-subscriptions": "^2.1.0",
    "graphql-subscriptions": "^1.0.0",
    "graphql-type-json": "^0.2.2",
    "handlebars": "^4.5.3",
    "https": "^1.0.0",
    "ioredis": "^4.9.0",
    "jest": "^24.5.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.7.0",
    "koa-jwt": "^3.5.1",
    "lodash": "^4.17.19",
    "migrate-mongo": "^7.0.1",
    "mongoose": "^5.7.5",
    "mongoose-timestamp": "^0.6.0",
    "nodemailer": "^5.1.1",
    "path": "^0.12.7",
    "redis": "^2.8.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.3.4000"
  },
  "devDependencies": {
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-document-nodes": "1.8.3",
    "@graphql-codegen/typescript-mongodb": "1.8.3",
    "@graphql-codegen/typescript-resolvers": "1.8.3",
    "@types/mongodb": "^3.1.22",
    "@types/node": "^12.12.8",
    "mjml": "^4.4.1",
    "mjml-with-images-loader": "^2.0.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^23.10.5"
  },
  "scripts": {
    "build": "tsc",
    "start": "ts-node ./src/server.ts",
    "start-inspect": "node --inspect=0.0.0.0 -r ts-node/register ./src/server.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "dev-inspect": "nodemon --exec npm run start-inspect",
    "prod": "npm run build && npm run start",
    "test": "jest",
    "migrate-db:up": "migrate-mongo up",
    "migrate-db:down": "migrate-mongo down",
    "migrate-db:create": "migrate-mongo create",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
