{
  "name": "@bitbloq/stl2bitbloqjson",
  "version": "0.1.3",
  "description": "Convert stl file to bitbloq object3D JSON file",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node --files ./src/index.ts",
    "dev": "./node_modules/nodemon/bin/nodemon.js",
    "prod": "npm run build && npm run start",
    "test": "echo \"No test specified\" && exit 0",
    "coveralls": "echo \"No test specified\" && exit 0",
    "tslint:dev": "tslint -c ./tslint-dev.json \"**/*.ts\"",
    "tslint:prod": "tslint -c ./tslint-prod.json \"**/*.ts\"",
    "prettier": "prettier --write \"**/*.{ts,js,tsx,jsx,json,md,scss,yml}\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bitbloq/bitbloq.git"
  },
  "keywords": [
    "STL",
    "3D"
  ],
  "author": "Alberto Valero <alberto.valero@bq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitbloq/bitbloq/issues"
  },
  "homepage": "https://github.com/bitbloq/bitbloq#readme",
  "moduleFileExtensions": [
    "ts",
    "json"
  ],
  "dependencies": {
    "@bitbloq/lib3d": "^0.1.3",
    "file-type": "^10.7.0",
    "jsdom-worker": "^0.1.0",
    "lodash": "^4.17.11",
    "node-fetch": "^2.3.0",
    "read-chunk": "^3.0.0",
    "ts-node": "^7.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@types/file-type": "^10.6.0",
    "@types/node": "^10.12.18",
    "@types/read-chunk": "^2.1.0",
    "prettier": "^1.15.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.4"
  },
  "gitHead": "768eeb61c740b95d4b78ad4c92a9313986806aba"
}
